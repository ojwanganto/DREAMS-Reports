	<queryString>
		<![CDATA[SELECT
	i.name as 'IP',
	COUNT(*) as 'Number of clients'
FROM
	DreamsApp_client c
INNER JOIN
	DreamsApp_implementingpartner i
ON
	c.implementing_partner_id = i.id
GROUP BY
	i.name]]>
